{
  "name": "@vibing-ai/cli",
  "version": "0.1.1",
  "description": "CLI tool for developing applications for Vibing AI",
  "main": "dist/index.js",
  "bin": {
    "vibe": "bin/vibe.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "prepare": "npm run build",
    "bundle": "pkg . --out-path=dist-bin",
    "bundle:all": "pkg . --targets=node18-macos-x64,node18-linux-x64,node18-win-x64 --out-path=dist-bin"
  },
  "files": [
    "dist",
    "bin",
    "templates"
  ],
  "pkg": {
    "assets": [
      "dist/**/*",
      "templates/**/*"
    ],
    "targets": [
      "node18"
    ],
    "outputPath": "dist-bin"
  },
  "keywords": [
    "cli",
    "ai",
    "marketplace",
    "vibe",
    "development"
  ],
  "author": "AI Developer",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^10.0.0",
    "fs-extra": "^11.1.1",
    "inquirer": "^12.6.0",
    "open": "^8.4.2",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^8.2.6",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.17.16",
    "@types/node": "^18.15.11",
    "jest": "^29.5.0",
    "pkg": "^5.8.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
